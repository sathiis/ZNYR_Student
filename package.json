{
  "name": "jitsi-meet-electron",
  "version": "1.1.1",
  "description": "ZNYR Student App",
  "main": "./build/main.js",
  "productName": "ZNYR Student App",
  "scripts": {
    "start": "webpack --config ./webpack.main.js  --mode development && concurrently \"npm:watch\" \"electron ./build/main.js\"",
    "clean": "rm -rf node_modules build dist",
    "lint": "eslint . && flow",
    "build": "webpack --config ./webpack.main.prod.js --mode production && webpack --config ./webpack.renderer.prod.js --mode production",
    "pack": "npm run build && electron-builder --publish always ",
    "dist": "npm run build && electron-builder",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "validate": "npm ls",
    "watch": "webpack --config ./webpack.renderer.js --mode development --watch --watch-poll"
  },
  "build": {
    "appId": "com.znyr.education",
    "productName": "ZNYR Student",
    "copyright": "Copyright Â© 2020 ZNYR",
    "generateUpdatesFilesForAllChannels": true,
    "files": [
      "**/*",
      "resources",
      "!app",
      "!main.js"
    ],
    "target": "NSIS",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "license": "app/license.txt"
    },
    "win": {
      "icon": "./Znyr.jpg",
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": {
      "provider": "github",
      "owner": "sathiis",
      "repo": "ZNYR_Student",
      "private": false
    }
  },
  "pre-commit": [
    "lint"
  ],
  "publish": {
    "provider": "github",
    "owner": "sathiis",
    "repo": "ZNYR_Student",
    "private": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sathiis/ZNYR_Student.git"
  },
  "keywords": [
    "jingle",
    "webrtc",
    "xmpp",
    "electron",
    "jitsi-meet"
  ],
  "author": "ZNYR",
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
